{
    "name": "personal-branding-back",
    "version": "0.0.1",
    "description": "Personal brading blog backend server with express, graphql, passport, typescript",
    "keywords": [
        "blog",
        "mysql",
        "graphql",
        "express"
    ],
    "main": "src/app.ts",
    "scripts": {
        "start": "nodemon",
        "build": "babel src --out-dir dist --copy-files --extensions .ts,.js",
        "fix:tslint": "tslint --project tsconfig.json --fix ",
        "fix:prettier": "prettier --write \"src/**/*.{ts,js]\"",
        "fix": "yarn fix:prettier &s yarn fix:tslint",
        "codegen:init": "graphql-codegen init",
        "codegen:generate": "graphql-codegen --config codegen.yml",
        "migrate:generate": "sequelize-cli migration:generate --name",
        "test": "jest"
    },
    "author": {
        "name": "Stark Jeon",
        "email": "a01058406602@gmail.com"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Jeontaeyun/personal-blog-graphql"
    },
    "license": "MIT",
    "dependencies": {
        "@types/jest": "^25.1.2",
        "apollo-server": "^2.25.4",
        "apollo-server-express": "^2.10.0",
        "bcrypt-nodejs": "0.0.3",
        "cookie-parser": "^1.4.4",
        "cors": "^2.8.5",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "express-session": "^1.16.2",
        "graphql": "^14.5.6",
        "graphql-import": "^1.0.0-beta.1",
        "http": "0.0.0",
        "https": "^1.0.0",
        "jsonwebtoken": "^8.5.1",
        "morgan": "^1.9.1",
        "mysql2": "^1.7.0",
        "passport": "^0.4.0",
        "passport-github": "^1.1.0",
        "passport-local": "^1.0.0",
        "sequelize": "^5.18.4",
        "sequelize-cli": "^5.5.1",
        "ts-node": "^8.5.4",
        "typedi": "^0.8.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.7.7",
        "@babel/core": "^7.7.7",
        "@babel/node": "^7.7.7",
        "@babel/plugin-proposal-class-properties": "^7.8.3",
        "@babel/plugin-proposal-decorators": "^7.8.3",
        "@babel/plugin-transform-arrow-functions": "^7.8.3",
        "@babel/plugin-transform-typescript": "^7.7.4",
        "@babel/preset-env": "^7.7.7",
        "@babel/preset-typescript": "^7.7.7",
        "@graphql-codegen/cli": "^1.12.2",
        "@graphql-codegen/introspection": "1.12.2",
        "@graphql-codegen/typescript": "1.12.2",
        "@graphql-codegen/typescript-document-nodes": "1.12.2",
        "@graphql-codegen/typescript-resolvers": "1.12.2",
        "@types/bluebird": "^3.5.29",
        "@types/cookie-parser": "^1.4.2",
        "@types/dotenv": "^8.2.0",
        "@types/express": "^4.17.2",
        "@types/express-session": "^1.15.16",
        "@types/graphql": "^14.5.0",
        "@types/morgan": "^1.7.37",
        "@types/node": "^13.1.2",
        "@types/passport": "^1.0.2",
        "@types/passport-github": "^1.1.5",
        "@types/sequelize": "^4.28.8",
        "@types/validator": "^12.0.1",
        "apollo-server-testing": "^2.9.4",
        "babel-plugin-module-resolver": "^4.0.0",
        "graphql-tools": "^4.0.6",
        "nodemon": "^1.19.2",
        "tslint": "^5.20.1",
        "tslint-config-prettier": "^1.18.0",
        "typescript": "^3.7.4"
    },
    "prettier": {
        "printWidth": 120,
        "tabWidth": 4,
        "singleQuote": false,
        "trailingComma": "none",
        "bracketSpacing": true,
        "semi": true,
        "useTabs": false,
        "arrowParens": "avoid",
        "endOfLine": "lf"
    }
}
